package modelo;

public class Array {

	@SuppressWarnings("unused")
	public static void main(String[] args) {
		
		
		ContaCorrente[] contas = new ContaCorrente[5];
		
	//	ContaPoupanca[] contas = new ContaPoupanca[5];
		
		ContaCorrente cc1 = new ContaCorrente(551, 876586);
		
		ContaPoupanca cp2 = new ContaPoupanca(551, 54543784);
		contas[0] =  cc1;
	//	contas[1] = cp2;
		
		System.out.println(cp2.getNumero());
		System.out.println(contas[1].getAgencia());
		System.out.println(contas[0].getAgencia());
	}

}
